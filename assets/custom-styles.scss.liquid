.cart_content{
  top: 100px;
  right: 20px;
  border: 1px solid rgba(162,162,162,0.3);
  right: 20px;
  border: 1px solid rgba(162,162,162,0.3);
  display: none;
  padding: 0 10px;
  box-shadow: 2px 16px 35px -17px rgba(162,162,162,0.3);
  @media screen and (max-width: 768px) {
    z-index: 1000000000;
    position: fixed;
    top: 0;
    right: 0;
    max-width: 400px;
    width: calc(100% - 10px);
    height: 100vh;
  }
  &.active_link{
    display: block;
  }
  .heading{
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 20px;
    @media screen and (max-width: 330px){
      padding: 20px 5px;
    }
    p{
      font-size: 14px;
      line-height: 16px;
      opacity: 0.3;
      margin: 0;
    }
    .close{
      position: relative;
      display: flex;
      align-items: center;
      justify-content: center;
      cursor: pointer;
      &::after{
        content: " ";
        width: 15px;
        transform: rotate(45deg);
        display: table;
        border-bottom: 1px solid black;
      }
      &::before{
        position: absolute;
        content: " ";
        transform: rotate(-45deg);
        width: 15px;
        border-bottom: 1px solid black;
        display: table;
      }
    }
  }
  &.overflow-items{
    hr{
      display: none;
    }
    .bottom-content{
      box-shadow: 0px -37px 28px -24px rgba(37,37,37,0.24);
    }
  }
  .js-cart_content__form {
    margin: 0;
    .progress-bar{
      margin: 0 20px 20px;
      padding-left: 20px;
      background: rgba(63,63,68,0.05);
      font-size: 11px;
      position: relative;
      overflow: hidden;
      @media screen and (max-width: 768px){
          font-size: 11px;
          padding-left: 10px;
          margin: 0 10px 20px;
      }
      @media screen and (max-width: 330px){
        margin: 0 5px 20px;
        font-size: 10px;
      }
      .progress{
        position: absolute;
        height: 100%;
        background: #50BCEA;
        left: 0;
        opacity: 0.4;
      }
    }
    .cart_items {
      max-height: 390px;
      overflow: auto;
      @media screen and (max-width:768px){
        height: calc(100vh - 360px);
        max-height: initial;
        overflow: auto;
      }
      @media screen and (max-width: 330px){
        padding: 15px 5px;
      }
      .cart_item {
        position: relative;
        margin-bottom: 20px;
        .cart_image{
          max-width: 90px;
          @media screen and (max-width: 768px){
            max-width: 80px;
          }
        }
        .cart_item__title{
          display: flex;
          flex-direction: column;
          .item_title{
            line-height: 16px;
          }
          @media screen and (max-width:768px) {
            line-height: 1.4;
            .item_title{
              min-height: 30px;
            }
          }
          .product-quantity-box{
            margin: 10px 0 5px;
            @media screen and (max-width:768px) {
              width: 110px;
            }
            .js-change-quantity{
              background: transparent;
              span{
                line-height: 27px;
                &::before{
                  font-size: 10px;
                  font-weight: 700;
                }
              }
            }
            input{
              font-size: 12px;
              width: 50%;
              border-left: none;
              border-right: none;
              color: #3C4144;
              z-index: -1;
            }
          }
          .cart__remove-btn{
            opacity: 0.3;
            line-height: 16px;
            cursor: pointer;
          }
        }
        .price{
          display: flex;
          position: absolute;
          right: 0;
          bottom: 32px;
          font-size: 14px;
          font-weight: 700;
          color: #3C4144;
          @media screen and (max-width: 768px){
            flex-direction: column;
          }
          .was_price{
            font-size: 14px;
            margin-right: 10px;
          }
        }
      }
    }
    hr{
      border-color: #EEEEEF;
      margin: 0 20px;
    }
    ul{
      @media screen and (max-width: 330px){
        padding-left: 5px;
        padding-right: 5px;
      }
    }
    .cart_subtotal, .cart_savings { 
      margin: 0;
      span{
        font-size: 15px;
        font-weight: 400;
        color: #32373b;
        &.right{
          span{
            font-weight: 700;
            font-size: 17px;
          }
        }
      }
    }
    .action_button{
      margin-top: 10px;
      font-size: 14px;
      text-transform: unset;
      @media screen and (max-width: 768px){
        margin-bottom: 0!important;
      }
    }
    .button{
      @media screen and (max-width: 768px){
        margin-top: 10px;
        display: flex!important;
        background: transparent;
        border-color: #BCBEBF;
        text-transform: unset;
      }
    }
  }
  .bottom{
    display: flex;
    justify-content: space-between;
    p{
      display: flex;
      align-items: center;
      font-size: 14px;
      margin: 0;
      @media screen and (max-width: 768px){
